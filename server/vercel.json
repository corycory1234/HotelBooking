{
    "version": 2,
    "function": {
        "api/**/*.ts": {
            "maxDuration": 10,
            "memory": 1024
        }
    },
    "builds": [
        {
            "src": "dist/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dist/index.js"
        }
    ]
}